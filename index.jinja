    <style>
        :root {
            --color-primary: #393939;
            --color-secondary: #ecf0f1;
            --color-icon: #535c68;
        }

        html {
            background-color: white;
            color: var(--color-primary);
            display: flex;
            align-items: center;
            justify-content: center;
        }

        body {
            width: 768px;
            font-size: small;
            display: flex;
            flex-direction: column;
        }

        *,
        ::after,
        ::before {
            box-sizing: border-box;
            font-family: 'Arial';
        }

        ul {
            margin: .5em 0;
        }

        hr {
            border: none;
            border-top: 1px dashed var(--color-secondary);
        }

        a {
            text-decoration: none;
        }

        a:hover {
            text-decoration: underline;
        }

        a:visited {
            color: blue;
        }

        fieldset {
            border: none;
        }

        .padded {
            display: grid;
            grid-template-columns: auto 1fr;
            grid-template-rows: 1fr;
            grid-column-gap: 10px;

            font-size: smaller;
            padding-left: 5em;
            width: 100%;
        }

        .bordered {
            border: .25px solid var(--color-secondary);
        }

        fieldset>legend {
            text-decoration: underline;
        }

        .kv {
            display: flex;
            justify-content: space-between;
            padding: .75rem .5rem;
        }

        .highlight {
            background-color: var(--color-secondary);
        }

        .social-icon {
            height: 10px;
            width: 10px;
        }

        .social-icon path {
            fill: var(--color-icon);
        }

        section#info div {
            display: grid;
            grid-template-columns: 1fr 1fr;
        }

        @media print {
            .highlight {
                background-color: var(--color-secondary);
            }
        }
    </style>
</head>

